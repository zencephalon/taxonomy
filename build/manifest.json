{
  "name": "taxonomy",
  "version": "0.0.0",
  "description": "An extension that helps U organize your bookmarks systematically",
  "permissions": [
    "bookmarks",
    "tabs"
  ],
  "browser_action": {
      "default_title": "taxonomy",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+K",
        "mac": "Ctrl+Shift+K"
      }
    }
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
